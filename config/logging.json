{
  "realTime": {
    "enabled": true,
    "level": "info",
    "transports": [
      "console",
      "file",
      "webhook"
    ]
  },
  "rotation": {
    "enabled": true,
    "maxFiles": 10,
    "maxSize": "10MB",
    "datePattern": "YYYY-MM-DD"
  },
  "structured": {
    "enabled": true,
    "format": "json",
    "includeStack": true
  }
}