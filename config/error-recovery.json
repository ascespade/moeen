{
  "autoRetry": {
    "enabled": true,
    "maxAttempts": 3,
    "delay": 5000,
    "backoffMultiplier": 2
  },
  "circuitBreaker": {
    "enabled": true,
    "failureThreshold": 5,
    "timeout": 60000,
    "resetTimeout": 30000
  },
  "fallback": {
    "enabled": true,
    "defaultResponse": {
      "success": false,
      "error": "Service temporarily unavailable",
      "retryAfter": 60
    }
  }
}