{
  "name": "Auto-Fix Agent System",
  "version": "2.0.0",
  "description": "Intelligent auto-fix system for errors and warnings",
  "features": {
    "lint_fix": true,
    "type_fix": true,
    "import_fix": true,
    "unused_vars_removal": true,
    "formatting": true,
    "build_check": true,
    "test_fix": true
  },
  "checks": ["typescript", "eslint", "prettier", "imports", "build"],
  "fix_strategies": {
    "missing_imports": "auto_add",
    "unused_vars": "remove",
    "type_errors": "infer_or_any",
    "lint_warnings": "auto_fix",
    "formatting": "prettier"
  },
  "max_iterations": 5,
  "abort_on_critical": false,
  "create_backup": true,
  "log_level": "verbose"
}
