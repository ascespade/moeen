{
  "name": "muayin-assistant",
  "version": "1.0.0",
  "private": true,
  "description": "مُعين - المساعد الذكي لمركز الهمم | AI Assistant for Al-Hemam Center",
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "ai-assistant",
    "healthcare",
    "whatsapp",
    "supabase",
    "gemini",
    "openai",
    "chatbot",
    "medical-assistant"
  ],
  "author": "Mu3een Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "db:generate": "supabase gen types typescript --local > src/types/database.types.ts",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:seed": "tsx scripts/seed.ts",
    "whatsapp:setup": "tsx scripts/whatsapp-setup.ts",
    "ai:test": "tsx scripts/test-ai-integration.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.39.0",
    "@supabase/ssr": "^0.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "critters": "^0.0.23",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.468.0",
    "next": "15.5.4",
    "next-themes": "^0.4.4",
    "next-intl": "^3.22.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.53.0",
    "recharts": "^2.12.7",
    "simple-icons": "^15.16.0",
    "zod": "^3.23.8",
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.67.3",
    "axios": "^1.7.9",
    "socket.io-client": "^4.8.1",
    "socket.io": "^4.8.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "nodemailer": "^6.9.16",
    "cron": "^3.1.6",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^6.4.19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "preline": "^3.2.3",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2",
    "@playwright/test": "^1.49.1",
    "cypress": "^13.15.1",
    "@sentry/nextjs": "^8.45.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
