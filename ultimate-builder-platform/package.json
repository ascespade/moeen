{
  "name": "ultimate-builder-platform",
  "version": "1.0.0",
  "description": "Ultimate Builder Platform - منصة البناء الذكية المتقدمة",
  "main": "run.js",
  "type": "module",
  "scripts": {
    "start": "node run.js",
    "dev": "node run.js --dev",
    "build": "npm run build:web && npm run build:agents",
    "build:web": "cd web-interface && npm run build",
    "build:agents": "cd agents && npm run build",
    "test": "npm run test:agents && npm run test:web",
    "test:agents": "cd agents && npm test",
    "test:web": "cd web-interface && npm test",
    "deploy": "node core/deploy.js",
    "monitor": "node core/monitor.js",
    "agent:create": "node core/agent-creator.js",
    "agent:start": "node agents/intelligent-agent/index.mjs",
    "server": "node core/server.js",
    "settings": "node core/settings-manager.js",
    "config": "node settings.js",
    "quick": "node quick-start.js",
    "install:all": "npm install && cd web-interface && npm install && cd ../agents && npm install",
    "clean": "rm -rf node_modules logs backups temp reports",
    "reset": "npm run clean && npm run install:all"
  },
  "keywords": [
    "ai",
    "builder",
    "visual-development",
    "code-generation",
    "automation",
    "parallel-processing",
    "background-sync",
    "git-integration",
    "intelligent-agent",
    "self-healing",
    "continuous-development"
  ],
  "author": "Ultimate Builder Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "ws": "^8.13.0",
    "fs-extra": "^11.1.1",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^9.2.12",
    "figlet": "^1.6.0",
    "boxen": "^7.0.0",
    "gradient-string": "^2.0.2",
    "node-cron": "^3.0.2",
    "pm2": "^5.3.0",
    "cluster": "^0.7.7",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ultimate-builder/platform.git"
  },
  "bugs": {
    "url": "https://github.com/ultimate-builder/platform/issues"
  },
  "homepage": "https://github.com/ultimate-builder/platform#readme"
}