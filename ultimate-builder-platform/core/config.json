{
  "platform": {
    "name": "Ultimate Builder Platform",
    "version": "1.0.0",
    "port": 3000,
    "maxAgents": 256,
    "backgroundMode": true,
    "autoSync": true,
    "theme": "dark",
    "language": "ar",
    "rtl": true
  },
  "agents": {
    "intelligentAgent": {
      "enabled": true,
      "backgroundMode": true,
      "autoSync": true,
      "checkInterval": 30000,
      "syncInterval": 300000,
      "maxCycles": "infinity",
      "parallelism": {
        "enabled": true,
        "maxWorkers": 4
      }
    },
    "visualBuilder": {
      "enabled": true,
      "port": 3001,
      "autoSave": true,
      "exportFormats": ["html", "react", "vue", "angular"]
    },
    "codeAnalyzer": {
      "enabled": true,
      "languages": ["javascript", "typescript", "python", "go", "rust"],
      "checks": ["eslint", "prettier", "typescript", "security"]
    },
    "performanceOptimizer": {
      "enabled": true,
      "bundleAnalysis": true,
      "imageOptimization": true,
      "cssOptimization": true
    },
    "securityScanner": {
      "enabled": true,
      "vulnerabilityCheck": true,
      "dependencyCheck": true,
      "codeSecurityCheck": true
    }
  },
  "git": {
    "autoCommit": true,
    "autoPush": true,
    "commitMessage": "Auto-sync: {timestamp} - {fixes} fixes applied",
    "branch": "main",
    "remote": "origin",
    "backupBeforeModify": true
  },
  "backup": {
    "enabled": true,
    "interval": 300000,
    "retention": 7,
    "compression": true,
    "encryption": false,
    "location": "backups/"
  },
  "monitoring": {
    "enabled": true,
    "interval": 60000,
    "metrics": ["cpu", "memory", "disk", "network", "agents"],
    "alerts": {
      "cpu": 80,
      "memory": 85,
      "disk": 90,
      "network": 1000
    },
    "logging": {
      "level": "info",
      "file": true,
      "console": true,
      "maxSize": "10MB",
      "maxFiles": 5
    }
  },
  "web": {
    "title": "Ultimate Builder Platform",
    "theme": "dark",
    "language": "ar",
    "rtl": true,
    "animations": true,
    "responsive": true
  },
  "security": {
    "cors": {
      "enabled": true,
      "origins": ["*"]
    },
    "rateLimit": {
      "enabled": true,
      "windowMs": 900000,
      "max": 100
    },
    "helmet": {
      "enabled": true
    },
    "https": {
      "enabled": false,
      "cert": "",
      "key": ""
    }
  },
  "ai": {
    "enabled": true,
    "provider": "openai",
    "model": "gpt-4",
    "maxTokens": 4000,
    "temperature": 0.7,
    "apiKey": "",
    "baseURL": "https://api.openai.com/v1"
  },
  "database": {
    "primary": "mongodb",
    "caching": true,
    "connectionPool": 10,
    "mongodb": {
      "connectionString": "",
      "database": "ultimate_builder"
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "password": ""
    }
  },
  "cloud": {
    "provider": "vercel",
    "autoDeploy": true,
    "cdn": true,
    "vercel": {
      "token": "",
      "teamId": "",
      "projectId": ""
    },
    "aws": {
      "accessKeyId": "",
      "secretAccessKey": "",
      "region": "us-east-1"
    }
  }
}
